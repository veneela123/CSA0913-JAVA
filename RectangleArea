import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
	    Scanner s=new Scanner(System.in);
	    System.out.print("l:");
	    int l=s.nextInt();
	    System.out.print("b:");
	    int b=s.nextInt();
	    int area=l*b;
	    System.out.println("Area of rectangle:"+area);
	}
}
